{
  "name": "bookstore-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:staging": "cp env.staging .env.local && next dev --turbopack",
    "dev:production": "cp env.production .env.local && next dev --turbopack",
    "build": "next build --turbopack",
    "build:staging": "cp env.staging .env.local && next build --turbopack",
    "build:production": "cp env.production .env.local && next build --turbopack",
    "start": "next start",
    "start:staging": "cp env.staging .env.local && next start",
    "start:production": "cp env.production .env.local && next start",
    "lint": "eslint",
    "env:dev": "cp .env .env.local",
    "env:staging": "cp env.staging .env.local",
    "env:production": "cp env.production .env.local"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
